{
  "name": "raneto",
  "version": "0.17.0",
  "description": "Markdown powered Knowledgebase",
  "author": "Patric Gruber <me@patric-gruber.at>",
  "license": "MIT",
  "keywords": [
    "raneto",
    "knowledgebase",
    "markdown",
    "static",
    "site",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/patricgruber/Raneto.git"
  },
  "bugs": "https://github.com/patricgruber/Raneto/issues",
  "contributors": [
    "https://github.com/patricgruber/Raneto/graphs/contributors"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/patricgruber/Raneto/master/LICENSE"
    }
  ],
  "bin": "./bin/raneto",
  "main": "./app/index.js",
  "scripts": {
    "start": "DEBUG=raneto node example/server.js",
    "start_win": "set DEBUG=raneto&&node example/server.js",
    "test": "npm run lint && mocha --reporter spec test/*.js",
    "lint": "./node_modules/.bin/eslint bin/* app/**/*.js example/**/*.js test/*.js",
    "copy-libs": "mkdir themes/default/public/lib ; cp -R ./node_modules/bootstrap themes/default/public/lib/ && cp -R ./node_modules/jquery themes/default/public/lib/ && cp -R ./node_modules/popper.js themes/default/public/lib/ && cp -R ./node_modules/bootstrap-rtl themes/default/public/lib/ && cp -R ./node_modules/fitvids themes/default/public/lib/ && cp -R ./node_modules/highlightjs themes/default/public/lib/ && cp -R ./node_modules/masonry-layout themes/default/public/lib/ && cp -R ./node_modules/sweetalert2 themes/default/public/lib/ && cp -R ./node_modules/jquery-backstretch themes/default/public/lib/",
    "postinstall": "npm run copy-libs"
  },
  "engines": {
    "node": ">=10.22.1"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "bootstrap": "4.6.0",
    "bootstrap-rtl": "3.3.4",
    "commander": "7.2.0",
    "cookie-parser": "1.4.5",
    "debug": "4.3.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "extend": "3.0.2",
    "fitvids": "2.1.1",
    "fs-extra": "10.0.0",
    "glob": "7.1.7",
    "highlightjs": "9.16.2",
    "hogan-express": "0.5.2",
    "jquery": "3.6.0",
    "jquery-backstretch": "2.1.18",
    "js-yaml": "4.1.0",
    "lunr": "2.3.9",
    "lunr-languages": "1.4.0",
    "markdown-it": "12.0.6",
    "markdown-toc": "1.2.0",
    "marked": "2.0.3",
    "masonry-layout": "4.2.2",
    "moment": "2.29.1",
    "morgan": "1.10.0",
    "node-fetch": "2.6.1",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0",
    "popper.js": "1.16.1",
    "sanitize-filename": "1.6.3",
    "serve-favicon": "2.5.0",
    "sitemap": "7.0.0",
    "sweetalert2": "10.16.7",
    "underscore": "1.13.1",
    "underscore.string": "3.3.5",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "eslint": "7.26.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "mocha": "8.4.0",
    "pm2": "4.5.6",
    "supertest": "6.1.3"
  }
}
